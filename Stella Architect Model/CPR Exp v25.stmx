		<variables>
			<stock name="GwBudget">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>100</eqn>
				<inflow>GwInflow</inflow>
				<outflow>Pumping</outflow>
				<outflow>PumpingYO</outflow>
				<units>Cubic Meters</units>
			</stock>
			<flow name="Pumping">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>DesiredPumping*EffectOfGroundwaterOnPumping</eqn>
				<non_negative/>
				<units>Cubic Meters/Years</units>
			</flow>
			<flow name="GwInflow">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>IF TIME &lt; 0 THEN ConstantGroundwaterInflow ELSE ConstantGroundwaterInflow*(1+GroundwaterNoise)*EffectOfGWOnInflows</eqn>
				<non_negative/>
				<units>Cubic Meters/Years</units>
			</flow>
			<aux name="ConstantGroundwaterInflow">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>IF TreatmentDelay=1 THEN 10.25 ELSE 11</eqn>
				<units>Cubic Meters/Year</units>
			</aux>
			<stock name="Orchards">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>100</eqn>
				<inflow>Y3ToO</inflow>
				<outflow>ObsolescenceRate</outflow>
				<outflow>DiscardRate</outflow>
				<units>Hectares</units>
			</stock>
			<flow name="ObsolescenceRate">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>Orchards/AverageLifetime</eqn>
				<non_negative/>
				<units>Hectares/Year</units>
			</flow>
			<flow name="OrchardGrowth">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>MAX(0, Decision)</eqn>
				<non_negative/>
				<units>Hectares/Year</units>
			</flow>
			<stock name="Profit">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>10000</eqn>
				<inflow>ProfitRate</inflow>
				<units>$</units>
			</stock>
			<stock name="AvgWaterCost">
				<eqn>IndicatedWaterCost</eqn>
				<inflow>ChangeInWaterCost</inflow>
				<units>$/Cubic Meters</units>
			</stock>
			<flow name="ChangeInWaterCost">
				<eqn>(IndicatedWaterCost-AvgWaterCost)/TimeToChangeWaterCost</eqn>
				<units>$/Cubic Meters/Years</units>
			</flow>
			<flow name="ProfitRate">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<element subscript="Farmer1">
					<eqn>IF TIME &lt;=0 THEN 0 ELSE RevenuePunishment[Farmer1] - Costs[Farmer1] - F1PunishmentImpositionCost</eqn>
				</element>
				<element subscript="Farmer2">
					<eqn>IF TIME &lt;=0 THEN 0 ELSE RevenuePunishment[Farmer2] - Costs[Farmer2] -F2PunishmentImpositionCost</eqn>
				</element>
				<element subscript="Farmer3">
					<eqn>IF TIME &lt;=0 THEN 0 ELSE RevenuePunishment[Farmer3] - Costs[Farmer3] -F3PunishmentImpositionCost</eqn>
				</element>
				<element subscript="Farmer4">
					<eqn>IF TIME &lt;=0 THEN 0 ELSE RevenuePunishment[Farmer4] - Costs[Farmer4] -F4PunishmentImpositionCost</eqn>
				</element>
				<units>$/Year</units>
			</flow>
			<aux name="TimeToChangeWaterCost">
				<eqn>1</eqn>
				<units>Year</units>
			</aux>
			<aux name="IndicatedWaterCost">
				<eqn>WaterCostMax/((1+WaterCostSlope*EXP(WaterCostGrowthRate*(NormalGroundwater-WaterCostInflection)))^(1/WaterCostSensitivity))</eqn>
				<units>$/Cubic Meters</units>
			</aux>
			<aux name="WaterCostSensitivity">
				<eqn>0.5*INIT(1+UNIFORM(-0.05, 0.05))</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="NormalGroundwater">
				<eqn>Groundwater/GWThreshold</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="EffectOfGroundwaterOnPumping">
				<eqn>MinimumGroundwaterEffectOnPumping+(MaximumGroundwaterEffectOnPumping-MinimumGroundwaterEffectOnPumping)*TableForEffectOfGroundwaterOnPumping</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="GrowthDelay">
				<eqn>TreatmentDelay/3</eqn>
				<units>Years</units>
			</aux>
			<aux name="Decision">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<element subscript="Farmer1">
					<eqn>5</eqn>
				</element>
				<element subscript="Farmer2">
					<eqn>5</eqn>
				</element>
				<element subscript="Farmer3">
					<eqn>5</eqn>
				</element>
				<element subscript="Farmer4">
					<eqn>5</eqn>
				</element>
				<units>Hectares/Year</units>
			</aux>
			<flow name="DiscardRate">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>MIN( MAX ( 0 , - DELAY3(Decision, GrowthDelay*3) ) , Orchards/DiscardTime )</eqn>
				<non_negative/>
				<units>Hectares/Year</units>
			</flow>
			<aux name="DiscardTime">
				<eqn>1</eqn>
				<units>Years</units>
			</aux>
			<aux name="AverageLifetime">
				<doc>Alternative formulation</doc>
				<eqn>NormalAverageLifetime</eqn>
				<units>Years</units>
			</aux>
			<aux name="Production">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>RefProduction*((Orchards/RefOrchards)^(ElasticityOfOrchards)*(Pumping/RefWater)^(ElasticityOfWater))*(1+ProductionNoise)</eqn>
				<units>Tonnes</units>
			</aux>
			<aux name="Revenue">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>IF TIME &lt;0 THEN 0 ELSE  Production*Price+DiscardRate*OrchardUnitSellingPrice</eqn>
				<units>$/Year</units>
			</aux>
			<aux name="OrchardsCost">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>Orchards*OrchardUnitVariableCost+OrchardGrowth*OrchardUnitFixedCost</eqn>
				<units>$/Year</units>
			</aux>
			<aux name="OrchardUnitVariableCost">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>NormalOrchardUnitVariableCost*(1-MAX(0, CostNoise))</eqn>
				<units>$/Hectares/Year</units>
			</aux>
			<aux name="OrchardUnitFixedCost">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>NormalOrchardUnitFixedCost*(1-MAX(0, CostNoise))</eqn>
				<units>$/Hectares</units>
			</aux>
			<aux name="WaterCost">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>(Pumping+PumpingYO)*AvgWaterCost</eqn>
				<units>$/Year</units>
			</aux>
			<aux name="Costs">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>IF TIME &lt;0 THEN 0 ELSE OrchardsCost+WaterCost</eqn>
				<units>$/Year</units>
			</aux>
			<aux name="Price">
				<eqn>100</eqn>
				<units>$/Tonne/Year</units>
			</aux>
			<aux name="OrchardUnitSellingPrice">
				<eqn>30</eqn>
				<units>$/Hectares</units>
			</aux>
			<aux name="DesiredPumping">
				<dimensions>
					<dim name="Farmers"/>
				</dimensions>
				<eqn>Orchards*WaterRequired</eqn>
				<units>Cubic Meters/Year</units>
			</aux>
			<aux name="MinimumGroundwaterEffectOnPumping">
				<eqn>0</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="MaximumGroundwaterEffectOnPumping">
				<eqn>1</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="TableForEffectOfGroundwaterOnPumping">
				<eqn>Groundwater/INIT(Groundwater)</eqn>
				<gf>
					<xscale min="0" max="1"/>
					<yscale min="0" max="1"/>
					<ypts>0,0.00942264230698451,0.0132509719172607,0.0186055007685057,0.0260665610808871,0.036408609346018,0.0506405679085384,0.0700314138515298,0.096095666271209,0.130499249160434,0.174837388566269,0.230250630947245,0.296907260790272,0.373497517393184,0.457003011546296,0.542996988453704,0.626502482606817,0.703092739209728,0.769749369052755,0.825162611433731,0.869500750839567,0.903904333728791,0.92996858614847,0.949359432091462,0.963591390653982,0.973933438919113,0.981394499231494,0.986749028082739,0.990577357693016,1</ypts>
				</gf>
				<units>Dimensionless</units>
			</aux>
			<aux name="WaterRequired">
				<eqn>0.1</eqn>
				<units>Cubic Meters/Hectare/Year</units>
			</aux>
			<aux name="RefProduction">
				<eqn>1</eqn>
				<units>Tonnes</units>
			</aux>
			<aux name="Years">
				<eqn>TIME</eqn>
				<units>Years</units>
				<event_poster>
					<threshold direction="increasing" value="30" repeat="once">
						<event sim_action="stop" isee:kind="interface_only">
							<link target="interface:19"/>
							<text_box/>
						</event>
					</threshold>
					<threshold direction="increasing" value="0" no_stop="true" repeat="once">
						<event isee:for_role="1" isee:kind="interface_only">
							<link target="interface:14"/>
							<text_box/>
						</event>
					</threshold>
					<threshold direction="increasing" value="0" no_stop="true" repeat="once">
						<event isee:for_role="2" isee:kind="interface_only">
